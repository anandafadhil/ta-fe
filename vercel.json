{
    "version": 2,
    "builds": [
      {
        "src": "app/**/*.{js,jsx,ts,tsx}",
        "use": "@vercel/next"
      },
      {
        "src": "src/page.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      { "src": "/predictbulk/result", "dest": "/predictbulk/result" },
      { "src": "/predictsingular/predictform", "dest": "/predictsingular/predictform" },
      { "src": "/predictsingular/result", "dest": "/predictsingular/result" },
      { "src": "/statisticpage", "dest": "/statisticpage" },
      { "src": "/statisticpage/major", "dest": "/statisticpage/major" },
      { "src": "/statisticpage/university", "dest": "/statisticpage/university" },
      { "src": "/", "dest": "/src/page.js" }
    ]
  }
  